        "list": "cpp",
        "*.tcc": "cpp",
        "math.h": "c",
        "complex": "c",
        "valarray": "c",
        "cmath": "c",
        "time.h": "c",
        "iostream": "cpp",
        "ctype.h": "c",
        "*.ds_store": "c",
        "stack.h": "c",
        "cstdio": "c",
        "pthread.h": "c",
        "cdefs.h": "c",
